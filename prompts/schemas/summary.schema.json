{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["has_two_sentences", "evidence"],
  "properties": {
    "has_two_sentences": {"type": "boolean"},
    "history_sentence": {"type": ["string", "null"]},
    "exam_sentence": {"type": ["string", "null"]},
    "evidence": {
      "type": "array",
      "minItems": 1,
      "items": {"type": "array", "items": [{"type": "integer"},{"type": "integer"}], "minItems": 2, "maxItems": 2}
    }
  }
}

