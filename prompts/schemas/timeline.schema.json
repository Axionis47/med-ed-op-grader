{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["events", "conflicts"],
  "properties": {
    "events": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["type", "confidence", "placement", "evidence"],
        "properties": {
          "type": {"type": "string"},
          "time_text": {"type": ["string", "null"]},
          "t_offset_min": {"type": ["number", "null"]},
          "confidence": {"type": "number", "minimum": 0, "maximum": 1},
          "placement": {"type": "string"},
          "evidence": {
            "type": "array",
            "minItems": 1,
            "items": {"type": "array", "items": [{"type": "integer"},{"type": "integer"}], "minItems": 2, "maxItems": 2}
          }
        }
      }
    },
    "conflicts": {"type": "array"}
  }
}

